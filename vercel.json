{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/oembed",
      "destination": "/api/oembed"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/PL([a-zA-Z0-9_-]{16,})",
      "destination": "/api/og?playlist=PL$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
