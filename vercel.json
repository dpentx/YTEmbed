{
  "version": 2,
  "rewrites": [
    {
      "source": "/:playlist",
      "destination": "/api/og?list=:playlist",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(bot|crawler|spider|scrapy|facebook|twitter|discord|slack|whatsapp|telegram).*"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
